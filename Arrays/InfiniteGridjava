int MAX(int x,int y){
    return (x>y)?x:y;
}
int coverPoints(int* X, int n1, int* Y, int n2) {
    int len=0;
    int i;
    int startx,starty;
    if(n1==0) return 0;
    if(n1==1) return 0;
    startx=X[0];
    starty=Y[0];
    for (i = 1; i < n1; i++) {
        int x = abs(startx - X[i]);
        int y = abs(starty - Y[i]);
        startx = X[i];
        starty = Y[i];
        
        len += MAX(x,y);
    }
    return len;
    
    
}